    "scripts": {
        "webpack": "webpack --config webpack.prod.config.js",
        "webpack-watch": "webpack --config webpack.dev.config.js --watch --info-verbosity",
        "tsc": "tsc -p tsconfig.build.json",
        "--0": "",
        "format-all": "prettier --write \"./(src|jest)/**/*.{js,jsx,ts,tsx,json}\"",
        "lint": "eslint src/**/*.{ts,tsx}",
        "audit": "npm audit | grep -B 1 -A 10 High",
        "--1": "",
        "clean": "node build-help.js --clean",
        "build-res": "node build-help.js --copy-build-res",
        "build": "npm run tsc",
        "bundle-res": "node build-help.js --copy-bundle-res --xpackage",
        "bundle": "npm run webpack -- --xpackage",
        "bundle-watch": "npm run webpack-watch -- --xpackage",
        "server": "cd bundle && node server-main.js",
        "server-watch": "cd bundle && nodemon --exec node server-main.js",
        "test": "jest --verbose",
        "test-nocover": "jest --verbose --coverage=false",
        "--2": "",
        "start": "npm run server"
    },
